{
  "manifest_version": 3,
  "name": "Codeforces CP-31 Master",
  "version": "2.0",
  "description": "Instantly loads CP-31 problems from API without pagination.",
  "permissions": ["activeTab"],
  "host_permissions": ["https://codeforces.com/api/*"],
  "content_scripts": [
    {
      "matches": ["https://codeforces.com/problemset*"],
      "js": ["cp31_data.js", "content.js"],
      "css": ["style.css"]
    }
  ]
}
